  let (sym_cap, sym_ite, sym_usf, sym_cof, sym_ent, sym_cat, sym_dos) = sep_cap in

   (Camlparagraph_symbol_t.camlparagraph_symbol,
    Item_symbol_t.item_symbol,	
    Usagefile_symbol_t.usagefile_symbol,
    Contentfile_symbol_t.contentfile_symbol,	
    Entity_symbol_t.entity_symbol, 
    Category_symbol_t.category_symbol,
    Domainset_symbol_t.domainset_symbol
   ) Septuplet_t.septuplet,
