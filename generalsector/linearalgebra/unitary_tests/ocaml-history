let nonzero_of_float_at_int_of_int f i len = 
  if i = 0
  then
    f :: zero (len-1)
  else
    if i = len
    then
      zero (len-1) @ [f]
    else
      let v_1 = zero i in
      let v_2 = zero (len-i) in
      v_1 @ [f] @ v_2
