(** {3 The Tag functionalities for a fragment.} *)

(** {6 Making.} *)

val make :
Fragment_symbol_t.fragment_symbol ->
  Fragment_state_t.fragment_state ->
    Fragment_index_t.fragment_index ->
      Fragment_tag_t.fragment_tag
;;

