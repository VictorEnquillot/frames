argument_abbreviation ()
{
case ${1} in
	[a-z]_[a-z]+ ) res="$1";;
	[a-z][a-z]_[a-z]+ ) res="$1";;
	[a-z][a-z][a-z]_[a-z]+ ) res="$1";;
	aggregate  ) res="agg";;
	asymmetric_unit  ) res="asu";;
	atom  ) res="atm";;
	atom_fivetied ) res="afi";;
	atom_fourtied ) res="afo";;
	atom_onetied ) res="aon";;
	atom_onetied_conjugated ) res="aoc";;
	atom_onetied_double ) res="aod";;
	atom_onetied_single ) res="aos";;
	atom_onetied_triple ) res="aot";;
	atom_sixtied ) res="asi";;
	atom_threetied ) res="ath";;
	atom_threetied_conjugated_conjugated_conjugated  ) res="accc";;
	atom_threetied_double_double_double ) res="addd";;
	atom_threetied_single_conjugated_conjugated ) res="ascc";;
	atom_threetied_single_single_double ) res="assd";;
	atom_threetied_single_single_single ) res="asss";;
	atom_twotied ) res="atw";;
	atom_twotied_conjugated_conjugated ) res="acc";;
	atom_twotied_conjugated_double ) res="acd";;
	atom_twotied_double_double ) res="add";;
	atom_twotied_single_conjugated ) res="asc";;
	atom_twotied_single_single ) res="ass";;
	atom_twotied_single_double ) res="asd";;
	atom_twotied_single_triple ) res="ast";;
	atom_zerotied  ) res="aze";;
	atom_zerotied_anion  ) res="aza";;
	atom_zerotied_anion_minus_one  ) res="am1";;
	atom_zerotied_anion_minus_two  ) res="am2";;
	atom_zerotied_cation  ) res="acn";;
	atom_zerotied_cation_plus_one  ) res="ac1";;
	atom_zerotied_cation_plus_three  ) res="ac3";;
	atom_zerotied_cation_plus_two  ) res="ac2";;
	atom_zerotied_raregas  ) res="arg";; 
	block  ) res="blk";;
	block_onedoublebifork ) res="bdb";;
 	block_zerozero  ) res="bzz";;
 	block_zerozero_diatomic  ) res="bzzd";;
 	block_zerozero_triatomic  ) res="bzzt";;
 	block_zerozero_tetratomic  ) res="bzzq";;
 	block_zerozero_pentatomic  ) res="bzzp";;
 	block_zeroone  ) res="bzo";;
	block_zeroonesingle  ) res="bzs";;
	block_zeroonedouble  ) res="bzd";;
	block_zeroonetriple  ) res="bzt";;
	block_zeroonehalfbridge  ) res="bzh";;
 	block_zerofork  ) res="bzf";;
 	block_zerobifork  ) res="bzb";;
 	block_zerotrifork  ) res="bzt";;
 	block_zeroquadrifork  ) res="bzq";;
	block_oneend  ) res="boe";;
 	block_onefork  ) res="bof";;
	block_onebifork  ) res="bob";;
	block_onetrifork  ) res="bot";;
	block_onesinglebifork  ) res="bsb";;
	block_onesingleonesingle  ) res="bss";;
	block_onesingleonedouble  ) res="bsd";;
	block_onedoubleonesingle  ) res="bds";;
	block_onedoubleonedouble  ) res="bdd";;
	block_onesingleonetriple  ) res="bst";;
	block_onetripleonesingle  ) res="bts";;	   
	block_onedoublebiforksinglesingle  ) res="bds2";;
	block_onesinglebiforksinglesingle  ) res="bss2";;
	block_onesingletriforksinglesinglesingle ) res="bss3";;
 	block_oneone  ) res="boo";;
 	block_onezero  ) res="boz";;
	bond  ) res="bnd";;
	box  ) res="box";;
	bridge  ) res="brg";;
	bridged_molecule ) res="bml";;
	caged_molecule ) res="cml";;
	chain  ) res="chn";;
	chemicalset  ) res="chs";;
	closed_fragment ) res="cfr";;
        cluster  ) res="clr";;
	cterminal_subunit ) res="cts";;
	cterminal_extended  ) res="ctx";;
	cterminal_leftextended  ) res="ctl";;
deoxyribonucleotide  ) res="dnc";;
deoxyribonucleotide_purinic  ) res="dpu";;
deoxyribonucleotide_pyrimidinic  ) res="dpy";;
ribonucleotide  ) res="rnc";;
ribonucleotide_purinic  ) res="rpu";;
ribonucleotide_pyrimidinic  ) res="rpy";;
	fragment  ) res="frg";;
     fragment_buried  ) res="frb";;
     fragment_closed  ) res="frc";;
     fragment_head  ) res="frh";;
     fragment_leaf  ) res="frl";;
	grouping  ) res="grp";;
	grouping_bridged  ) res="grb";;
	grouping_polyfragment  ) res="gpf";;
	halfbridge  ) res="hbd";;     
	mendeleev  ) res="mnd";;
	molecule  ) res="mol";;
	molecule_aminoacid  ) res="mam";;
	molecule_aminoacid_leftextended  ) res="mal";;
     molecule_aminoacid_regular  ) res="mar";;
molecule_aminoacid_fragmented ) res="maf";;
molecule_aminoacid_segmented ) res="mas";;
molecule_dendrimer ) res="mdn";;
molecule_forked_moreforked ) res="mfm";;
molecule_forked_oneforked ) res="mfo";;
molecule_forked_threeforked ) res="mf3";;
molecule_forked_twoforked ) res="mf2";;
     molecule_aminoacid_olc  ) res="amo";;
     molecule_aminoacid_fragmented_olc  ) res="aof";;
     molecule_aminoacid_segmented_olc  ) res="aos";;
     molecule_aminoacid_glycine_olc  ) res="aog";;
     molecule_aminoacid_proline_olc  ) res="aop";;
     molecule_aminoacid_regular_olc  ) res="aor";;
     molecule_aminoacid_tlc  ) res="amt";;
     molecule_aminoacid_fragmented_tlc  ) res="aft";;
     molecule_aminoacid_segmented_tlc  ) res="ast";;
     molecule_aminoacid_glycine_tlc  ) res="agt";;
     molecule_aminoacid_proline_tlc  ) res="apt";;
     molecule_aminoacid_regular_tlc  ) res="art";;
	molecule_linear  ) res="ml";;  
   molecule_linear_monoatomic  ) res="mlm";;
   molecule_linear_diatomic  ) res="ml2";;
   molecule_linear_triatomic  ) res="ml3";;
  molecule_linear_tetratomic  ) res="ml4";;
   molecule_linear_pentatomic  ) res="ml5";;
   molecule_linear_polyatomic  ) res="mlp";;
	molecule_polymer  ) res="mp";;
	monomer ) res="mon";;
	nitrogenous_base  ) res="nbs";;
	nucleicacid  ) res="nca";;
	nucleoside  ) res="ncs";;
	nucleotide  ) res="nct";;
	peptideca  ) res="pca";;
	peptideca_buried  ) res="pcb";;
	peptideca_nterminal  ) res="pcn";; 
     peptideca_buried_fragmented  ) res="pbf";;
     peptideca_buried_segmented  ) res="pbs";;
     peptideca_nterminal_fragmented  ) res="pnf";;
     peptideca_nterminal_segmented  ) res="pns";;	
     peptidechain  ) res="pch";;
	polymer ) res="pol";;
	polyglycine  ) res="pgl";;
	polyproline  ) res="ppr";;
	polypeptide  ) res="plp";;
	polypeptide_regular  ) res="plr";;
	polypeptide_sidegroup  ) res="pls";;
	protein  ) res="prn";;
purine_base  ) res="pur";;
pyrimidine_base  ) res="pyr";;
	residue  ) res="res";;
	residue_buried ) res="rse";;
	residue_nterminal ) res="rsn";;
     residue_buried_fragmented  ) res="rbf";;
     residue_buried_segmented  ) res="rbs";;
     residue_nterminal_fragmented  ) res="rnf";;
     residue_nterminal_segmented  ) res="rns";;	
     peptidechain  ) res="pch";;
	ringed_molecule ) res="tml";;
	segment  ) res="seg";;
	segment_buried ) res="sgb";;
	segment_head ) res="sgh";;
	sidechain  ) res="sdc";;
	sidegroup  ) res="sdg";;
     	aggregate ) res="agg";;
     	molecule_bridged  ) res="mbd";;
     	molecule_forked  ) res="mfk";;
      	*) 
	echo "";
	echo "Error in do_argument_abbreviation_all : unknown module >$1<";
	echo "";
      echo -n "   $1  ) res=\"\";;";	
    exit	 
	;;
esac
echo "$res"
}
